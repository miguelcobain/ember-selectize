{
  "name": "ember-selectize",
  "version": "0.1.0",
  "description": "An Ember and Selectize integration. ",
  "main": "src/ember.selectize.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "goat -e test/index.html bower_components/:/bower_components src/:/src",
    "postinstall": "bower install",
    "build": "mkdir dist && uglifyjs src/ember.selectize.js -m -c > dist/ember.selectize.min.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/miguelcobain/ember-selectize.git"
  },
  "keywords": [
    "ember",
    "selectize"
  ],
  "author": "Miguel Andrade <jmandrade0@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/miguelcobain/ember-selectize/issues"
  },
  "homepage": "https://github.com/miguelcobain/ember-selectize",
  "devDependencies": {
    "bower": "^1.3.5",
    "goat": "^0.3.0",
    "uglify-js": "^2.4.14"
  }
}
